                                                    <li><a asp-controller="BlogDetails" asp-action="Index">blog details</a></li>
                                                </ul>
                                            </li>
                                            <li><a asp-controller="Contact" asp-action="Index">Contact</a></li>
                                            <li><a href="#">Buy Now</a>
                                        </ul>
                                    </nav>
                                </div>
                                <div class="mobile-menu hidden-lg hidden-md one"></div>
                                <!-- Main Menu End -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
        else
        {
            <div class="header-top">
                <div class="container">
                    <div class="row">
                        <div class="col-md-8 col-sm-8 col-xs-12">
                            <div class="header-top-left">
                                <p>HAVE ANY QUESTION ?  @Model.Setting.QuestionPhone</p>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <div class="header-top-right text-right">
                                <ul>
                                    <li><a asp-controller="Login" asp-action="Index">login</a></li>
                                    <li><a asp-controller="Signup" asp-action="Index">signup</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="header-area two header-sticky">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-sm-3 col-xs-6">
                            <div class="logo">
                                <a asp-controller="Home" asp-action="Index"><img src="img/logo/@Model.Setting.Logo" alt="eduhome" /></a>
                            </div>
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-6">
                            <div class="content-wrapper text-right">
                                <!-- Main Menu Start -->
                                <div class="main-menu">
                                    <nav>
                                        <ul>
                                            <li>
                                                <a asp-controller="Home" asp-action="Index">Home</a>
                                                <ul>
                                                    <li><a asp-controller="Home" asp-action="Index">Home One</a></li>
                                                    <li><a asp-controller="Index_2" asp-action="Index">Home Two</a></li>
                                                    <li><a asp-controller="Index_3" asp-action="Index">Home Three</a></li>
                                                    <li><a asp-controller="Index_4" asp-action="Index">Home Four</a></li>
                                                    <li><a asp-controller="Index_5" asp-action="Index">Home Five</a></li>
                                                </ul>
                                            </li>
                                            <li><a asp-controller="About" asp-action="Index">About</a></li>
                                            <li>
                                                <a asp-controller="Course" asp-action="Index">courses</a>
                                                <ul>
                                                    <li><a asp-controller="Course" asp-action="Index">courses</a></li>
                                                    <li><a asp-controller="CourseDetails" asp-action="Index">courses details</a></li>
                                                </ul>
                                            </li>
                                            <li>
                                                <a asp-controller="Event" asp-action="Index">event</a>
                                                <ul>
                                                    <li><a asp-controller="Event" asp-action="Index">event</a></li>
                                                    <li><a asp-controller="EventLeftSidebar" asp-action="Index">event left sidebar</a></li>
                                                    <li><a asp-controller="EventRightSidebar" asp-action="Index">event right sidebar</a></li>
                                                    <li><a asp-controller="EventDetails" asp-action="Index">event details</a></li>
                                                </ul>
                                            </li>
                                            <li class="hidden-sm">
                                                <a asp-controller="Teacher" asp-action="Index">teacher</a>
                                                <ul>
                                                    <li><a asp-controller="Teacher" asp-action="Index">teacher</a></li>
                                                    <li><a asp-controller="TeacherDetails" asp-action="Index">teacher details</a></li>
                                                </ul>
                                            </li>
                                            <li>
                                                <a asp-controller="Blog" asp-action="Index">blog</a>
                                                <ul>
                                                    <li><a asp-controller="Blog" asp-action="Index">blog</a></li>
                                                    <li><a asp-controller="BlogLeftSidebar" asp-action="Index">blog left sidebar</a></li>
                                                    <li><a asp-controller="BlogRightSidebar" asp-action="Index">blog right sidebar</a></li>
                                                    <li><a asp-controller="BlogDetails" asp-action="Index">blog details</a></li>
                                                </ul>
                                            </li>
                                            <li><a asp-controller="Contact" asp-action="Index">Contact</a></li>
                                            <li><a href="#">Buy Now</a>
                                        </ul>
                                    </nav>
                                </div>
                                <!--Search Form Start-->
                                <div class="search-btn">
                                    <ul data-toggle="dropdown" class="header-search search-toggle">
